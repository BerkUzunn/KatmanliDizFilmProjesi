@model IEnumerable<KatmanlıModel.Model.Admin>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create" class="btn btn-success">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AdminId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdminAdı)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdminSifre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AdminId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminAdı)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminSifre)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.AdminId },new { @class = "btn btn-warning" }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.AdminId },new {@class="btn btn-danger"})
            </td>
        </tr>
}
    </tbody>
</table>
